{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue","src/**/*", "public/code/**/*"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "allowJs": true,  // 允许编译js
    "checkJs": false, // 不检查js编译的错误
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
